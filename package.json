{
    "name": "linkme",
    "version": "0.0.1",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "eslint": "npx eslint --ext .jsx,.js,.tsx,.ts ./ --fix",
        "prettier": "prettier --write \"*.+(json|css)\""
    },
    "dependencies": {
        "firebase": "7.19.1",
        "next": "9.5.4",
        "precise-ui": "1.4.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-hook-form": "6.8.1",
        "styled-components": "5.2.0",
        "styled-reset": "4.2.3"
    },
    "devDependencies": {
        "@types/node": "12.12.56",
        "@types/react": "16.9.49",
        "@types/react-dom": "16.9.8",
        "@types/styled-components": "5.1.3",
        "@typescript-eslint/eslint-plugin": "4.1.0",
        "@typescript-eslint/parser": "4.1.0",
        "babel-plugin-styled-components": "1.11.1",
        "eslint": "7.8.1",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.20.6",
        "husky": "4.3.0",
        "lint-staged": "10.3.0",
        "next-transpile-modules": "4.1.0",
        "prettier": "2.1.1",
        "typescript": "4.0.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{json,css}": [
            "prettier --write"
        ],
        "*.{js,ts,tsx,jsx}": [
            "eslint --fix"
        ]
    }
}
